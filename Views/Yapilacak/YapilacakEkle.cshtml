@model Online_Ticari_Otomasyon.Models.Siniflar.Yapilacak
@{
    ViewBag.Title = "YapilacakEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Yapılması Gerekenlere Ekleme Yap</h2>
<br />

<form class="form-group" method="post">
    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary" type="button">Yapılacak Notu Kaydet</button>
</form>
